{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "testflight": {
      "distribution": "store",
      "channel": "testflight",
      "ios": {
        "buildConfiguration": "Release"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://YOUR-API.up.railway.app"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://YOUR-API.up.railway.app"
      }
    }
  },
  "submit": {
    "testflight": {
      "ios": {}
    },
    "production": {
      "ios": {}
    }
  }
}
