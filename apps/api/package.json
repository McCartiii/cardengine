{
  "name": "@cardengine/api",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "build": "prisma generate && tsup src/index.ts --format esm",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push"
  },
  "engines": {
    "node": ">=20.19"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.15",
    "@fastify/cors": "^10.0.0",
    "@fastify/rate-limit": "^10.0.0",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/adapter-pg-worker": "^6.9.0",
    "@prisma/client": "^7.3.0",
    "dotenv": "^17.2.0",
    "fastify": "^5.2.0",
    "jose": "^6.0.0",
    "pg": "^8.18.0",
    "pglite-prisma-adapter": "^0.7.2",
    "prisma": "^7.3.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
